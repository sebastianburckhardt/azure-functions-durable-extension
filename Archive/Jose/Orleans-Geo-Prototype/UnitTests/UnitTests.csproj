    <PostBuildEvent>
      @echo UnitTest - Start Post-Build script

      if not exist "$(TargetDir)TestInput" ( mkdir "$(TargetDir)TestInput" )

      copy /y "$(SolutionDir)UnitTests\test.config"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)UnitTests\*.xml"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)OrleansMgmtUI\OrleansManager\Deployment.xml"  "$(TargetDir)TestInput\"

      @REM Ensure Azure RoleEnvironment.IsAvailable call does not complain about missing msshrtmi.dll
      copy /y "$(SolutionDir)Dependencies\Windows-Azure-SDK\v$(AzureSdkVersion)\ref\*.dll"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)Dependencies\Windows-Azure-SDK\v$(AzureSdkVersion)\bin\runtimes\base\x64\msshrtmi.dll"  "$(TargetDir)TestInput\"

      @echo Deployment Tests: copying to $(TargetDir)TestInput\Configuration
      if not exist "$(TargetDir)TestInput\Configuration" ( mkdir "$(TargetDir)TestInput\Configuration" )
      if not exist "$(TargetDir)TestInput\Configuration\TestAbsolutePaths" ( mkdir "$(TargetDir)TestInput\Configuration\TestAbsolutePaths" )
      if not exist "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans" ( mkdir "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans")
      if not exist "$(TargetDir)TestInput\Configuration\TestCleanOrleans" ( mkdir "$(TargetDir)TestInput\Configuration\TestCleanOrleans")
      if not exist "$(TargetDir)TestInput\Configuration\TestRelativePaths" ( mkdir "$(TargetDir)TestInput\Configuration\TestRelativePaths")

      copy /y "$(SolutionDir)Scripts\Deployment\*.ps1"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)UnitTests\DeploymentTests\Configuration\TestAbsolutePaths\*.*"  "$(TargetDir)TestInput\Configuration\TestAbsolutePaths"
      copy /y "$(SolutionDir)UnitTests\DeploymentTests\Configuration\TestCleanEmptyOrleans\*.*"  "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans"
      copy /y "$(SolutionDir)UnitTests\DeploymentTests\Configuration\TestCleanOrleans\*.*"  "$(TargetDir)TestInput\Configuration\TestCleanOrleans"
      copy /y "$(SolutionDir)UnitTests\DeploymentTests\Configuration\TestRelativePaths\*.*"  "$(TargetDir)TestInput\Configuration\TestRelativePaths"

      @echo UnitTests: copying to $(TargetDir)TestInput
      if not exist "$(TargetDir)TestInput\Applications" ( mkdir "$(TargetDir)TestInput\Applications" )
      if not exist "$(TargetDir)TestInput\Applications\UnitTests" ( mkdir "$(TargetDir)TestInput\Applications\UnitTests" )

      @echo UnitTests: copying test image files to $(TargetDir)TestInput
      copy /y "$(TargetDir)*.png"  "$(TargetDir)TestInput\"

      if "$(BuildingInsideVisualStudio)" == "true" (
      copy /y "$(SolutionDir)UnitTestGrains\bin\$(ConfigurationName)\UnitTestGrains.*"  "$(TargetDir)TestInput\"
      rem copy /y "$(SolutionDir)UnitTestCLIGrains\$(ConfigurationName)\UnitTestCLIGrains.*"  "$(TargetDir)TestInput\"
      rem copy /y "$(SolutionDir)UnitTestCLIGrains\$(ConfigurationName)\UnitTestCLIGrainsActivation.*"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)UnitTestGrainInterfaces\bin\$(ConfigurationName)\UnitTestGrainInterfaces.*"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)UnsignedUnitTestGrains\bin\$(ConfigurationName)\UnsignedUnitTestGrains.*"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)UnsignedUnitTestGrainInterfaces\bin\$(ConfigurationName)\UnsignedUnitTestGrainInterfaces.*"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)AssemblyLoaderTestAssembly\bin\$(ConfigurationName)\AssemblyLoaderTestAssembly.*"  "$(TargetDir)TestInput\"
      ) else (
      copy /y "$(TargetDir)\UnitTestGrains.*"  "$(TargetDir)TestInput\"
      copy /y "$(TargetDir)\UnitTestCLIGrains.*"  "$(TargetDir)TestInput\"
      copy /y "$(TargetDir)\UnitTestCLIGrainsActivation.*"  "$(TargetDir)TestInput\"
      copy /y "$(TargetDir)\UnitTestGrainInterfaces.*"  "$(TargetDir)TestInput\"
      copy /y "$(TargetDir)\UnsignedUnitTestGrains.*"  "$(TargetDir)TestInput\"
